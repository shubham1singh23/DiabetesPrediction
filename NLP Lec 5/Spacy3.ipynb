    "import spacy\n",
    "\n",
    "nlp=spacy.load(\"en_core_web_lg\")\n",
    "\n",
    "s1=input(\"Enter s1 :\")\n",
    "s2=input(\"Enter s2 :\")\n",
    "v1=nlp(s1)\n",
    "v2=nlp(s2)\n",
    "\n",
    "score=v1.similarity(v2)\n",
    "print(score)"
   ],
